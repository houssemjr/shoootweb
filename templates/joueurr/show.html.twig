<!DOCTYPE html>

<title>Joueur</title>

{% block body %}
    <h1>Joueur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdJoueur</th>
                <td>{{ joueur.idJoueur }}</td>
            </tr>
            <tr>
                <th>NomJoueur</th>
                <td>{{ joueur.nomJoueur }}</td>
            </tr>
            <tr>
                <th>Datenaiss</th>
                <td>{{ joueur.datenaiss ? joueur.datenaiss|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ joueur.age }}</td>
            </tr>
            <tr>
                <th>Pays</th>
                <td>{{ joueur.pays }}</td>
            </tr>
            <tr>
                <th>Drapeau</th>
                <td>{{ joueur.drapeau }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ joueur.image }}</td>
            </tr>
            <tr>
                <th>NomEquipe</th>
                <td>{{ joueur.nomEquipe }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_joueurr_index') }}">back to list</a>

    <a href="{{ path('app_joueurr_edit', {'idJoueur': joueur.idJoueur}) }}">edit</a>

    {{ include('joueurr/_delete_form.html.twig') }}
{% endblock %}
